source 'https://rubygems.org'

gem 'rails', '3.2.12'
gem 'sqlite3'
gem 'strong_parameters', '0.2.0'
gem 'monban', '0.0.6'
gem 'paperclip', '3.4.0'
gem 'awesome_print'

# groups allow you to clump gems together
# main benefit is you can bundle install 
# without a particular group:
# bundle --without assets
group :assets do
  gem 'sass-rails',   '~> 3.2.3'
  gem 'coffee-rails', '~> 3.2.1'
  gem 'uglifier', '>= 1.0.3'
end

gem 'jquery-rails'
